{
    "site_name": "BPI-Steam",
    "site_slogon": "BananaPi Steam Education Team",
    "site_root_url": "/",
    "site_domain": "bpi-steam.com",
    "site_protocol": "https",
    "config_template_dir": "config",
    "source": "https://github.com/BPI-STEAM/BPI-Doc/blob/main",
    "route": {
        "docs": {
            "/Q-Car/zh/": "docs/Q-Car/zh",
            "/Triode-Car/zh/": "docs/Triode-Car/zh",
            "/WebBit_dev/zh/": "docs/WebBit_dev/zh",
            "/WebBit_doc/zh/": "docs/WebBit_doc/zh",
            "/WebAI/zh/": "docs/WebAI/zh",
            "/About/zh/": "docs/About/zh"
            
        },
        "pages": {
            "/": "pages/index/zh"
        },
        "assets": {
            "/static/": "static",
            "/Q-Car/zh/assets/":"docs//Q-Car/zh/assets",
            "/Triode-Car/zh/assets/": "docs/Triode-Car/zh/assets",
            "/WebAI/zh-tw/assets/": "docs/WebAI/zh-tw/assets",
            "/WebAI/zh/assets/": "docs/WebAI/zh/assets",
            "/WebBit_dev/zh/assets/": "docs/WebBit_dev/zh/assets",
            "/WebBit_doc/zh/assets/": "docs/WebBit_doc/zh/assets"
        },
        "blog": {
            "/news/": "news"
        }
    },
    "translate": {
        "docs": {
            "/Q-Car/zh/": [
                {
                    "url": "/Q-Car/en/",
                    "src": "docs/Q-Car/en"
                },
                {
                    "url": "/Q-Car/zh-tw/",
                    "src": "docs/Q-Car/zh-tw"
                }
            ],
            "/Triode-Car/zh/": [
                {
                    "url": "/Triode-Car/zh-tw/",
                    "src": "docs/Triode-Car/zh-tw"
                },
                {
                    "url": "/Triode-Car/en/",
                    "src": "docs/Triode-Car/en"
                }
            ],
            "/WebBit_dev/zh/": [
                {
                    "url": "/WebBit_dev/zh-tw/",
                    "src": "docs/WebBit_dev/zh-tw"
                },
                {
                    "url": "/WebBit_dev/en/",
                    "src": "docs/WebBit_dev/en"
                }
            ],
            "/WebBit_doc/zh/": [
                {
                    "url": "/WebBit_doc/zh-tw/",
                    "src": "docs/WebBit_doc/zh-tw"
                },
                {
                    "url": "/WebBit_doc/en/",
                    "src": "docs/WebBit_doc/en"
                }
            ],
            "/WebAI/zh/": [
                {
                    "url": "/WebAI/zh-tw/",
                    "src": "docs/WebAI/zh-tw"
                },
                {
                    "url": "/WebAI/en/",
                    "src": "docs/WebAI/en"
                }
            ],
            "/About/zh/": [
                {
                    "url": "/About/zh-tw/",
                    "src": "docs/About/zh-tw"
                },
                {
                    "url": "/About/en/",
                    "src": "docs/About/en"
                }
            ]
        },
        "pages": {
            "/": [
                {
                    "url": "/zh-tw/",
                    "src": "pages/index/zh-tw"
                },
                {
                    "url": "/en/",
                    "src": "pages/index/en"
                }
            ]
        }
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
                "toc_depth": 4
            }
        },
        "teedoc-plugin-jupyter-notebook-parser": {
            "from": "pypi",
            "config": {

            }
        },
        "teedoc-plugin-google-translate": {
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-blog": {
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "env":{
                    "main_color": "#225378"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "Search",
                "env":{
                    "main_color": "#225378",
                    "main_color_dark": "#923732",
                    "hint_shadow_color": "rgba(146, 55, 50, 0.38)"
                }
            }
        },
        "teedoc-plugin-baidu-tongji":{
            "from": "pypi",
            "config": {
                "code": "9cb07365544a53067c56c346c838181a"
            }
        },
        "teedoc-plugin-google-analytics":{
            "from": "pypi",
            "config": {
                "id": "UA-119047820-5"
            }
        },
        "teedoc-plugin-comments-gitalk": {
            "from": "pypi",
            "config": {
                "env": {
                    "clientID": "13d7605e4abe8192405c",
                    "clientSecret": "105f1bf3dee8f3b8c0cecfcfbdb1c273a3babb12",
                    "repo": "sipeed_wiki",
                    "owner": "sipeed",
                    "admin": ["Neutree", "STRfarfar"],
                    "main_color": "#225378",
                    "second_color": "#e25d55"
                }
            }
        },
        "teedoc-plugin-ad-hint": {
            "from": "pypi",
            "config": {
                "type": "hint",
                "label": "☆",
                "content": "新版 MaixHub beta 测试版已上线，支持在线标注、一键部署，更稳定的网络和流畅的体验，并增加 Maix-II-Dock 支持<br/><a href='https://maix.sipeed.com'>快来体验吧！</a>",
                "show_after_s": 432000,
                "date": "2022-06-01 14:00"
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env":{
                    "main_color": "#225378",
                    "second_color": "#e25d55"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "teedoc-plugin-google-translate": {
        "from": "pypi",
        "config": {
        }
    }
}
