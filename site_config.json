{
    "site_name": "BPI-Steam",
    "site_slogon": "BananaPi Steam Education Team",
    "site_root_url": "/",
    "site_domain": "bpi-steam.com",
    "site_protocol": "https",
    "config_template_dir": "config",
    "source": "https://github.com/BPI-STEAM/BPI-Doc/blob/main",
    "route": {
        "docs": {
            "/Q-Car/zh/": "docs/Q-Car/zh",
            "/Triode-Car/zh/": "docs/Triode-Car/zh",
            "/WebBit_dev/zh/": "docs/WebBit_dev/zh",
            "/WebBit_doc/zh/": "docs/WebBit_doc/zh",
            "/Bit_S2_doc/zh/": "docs/Bit_S2_doc/zh",
            "/WebAI/zh/": "docs/WebAI/zh",
            "/Centi_S3_doc/zh/": "docs/Centi_S3_doc/zh",
            "/Leaf_S3_doc/zh/": "docs/Leaf_S3_doc/zh",
            "/PicoW_S3_doc/zh/": "docs/PicoW_S3_doc/zh",
            "/Pico_RP2040_doc/zh/": "docs/Pico_RP2040_doc/zh",
            "/About/zh/": "docs/About/zh"
            
        },
        "pages": {
            "/": "pages/index/en"
        },
        "assets": {
            "/static/": "static",
            "/Q-Car/zh/assets/":"docs/Q-Car/zh/assets",
            "/Centi_S3_doc/zh/assets/":"docs/Centi_S3_doc/zh/assets",
            "/Leaf_S3_doc/zh/assets/":"docs/Leaf_S3_doc/zh/assets",
            "/PicoW_S3_doc/zh/assets/":"docs/PicoW_S3_doc/zh/assets",
            "/Pico_RP2040_doc/zh/assets/":"docs/Pico_RP2040_doc/zh/assets",
            "/Triode-Car/zh/assets/": "docs/Triode-Car/zh/assets",
            "/WebAI/zh_tw/assets/": "docs/WebAI/zh_tw/assets",
            "/WebAI/zh/assets/": "docs/WebAI/zh/assets",
            "/WebBit_dev/zh/assets/": "docs/WebBit_dev/zh/assets",
            "/WebBit_doc/zh/assets/": "docs/WebBit_doc/zh/assets",
            "/Bit_S2_doc/zh/assets/": "docs/Bit_S2_doc/zh/assets",
            "/news/assets/": "news/assets"
        },
        "blog": {
            "/news/": "news"
        }
    },
    "translate": {
        "docs": {
            "/Q-Car/zh/": [
                {
                    "url": "/Q-Car/en/",
                    "src": "docs/Q-Car/en"
                },
                {
                    "url": "/Q-Car/zh_tw/",
                    "src": "docs/Q-Car/zh_tw"
                }
            ],
            "/Triode-Car/zh/": [
                {
                    "url": "/Triode-Car/zh_tw/",
                    "src": "docs/Triode-Car/zh_tw"
                },
                {
                    "url": "/Triode-Car/en/",
                    "src": "docs/Triode-Car/en"
                }
            ],
            "/Centi_S3_doc/zh/": [
                {
                    "url": "/Centi_S3_doc/zh_tw/",
                    "src": "docs/Centi_S3_doc/zh_tw"
                },
                {
                    "url": "/Centi_S3_doc/en/",
                    "src": "docs/Centi_S3_doc/en"
                }
            ],
            "/Leaf_S3_doc/zh/": [
                {
                    "url": "/Leaf_S3_doc/zh_tw/",
                    "src": "docs/Leaf_S3_doc/zh_tw"
                },
                {
                    "url": "/Leaf_S3_doc/en/",
                    "src": "docs/Leaf_S3_doc/en"
                }
            ],
            "/PicoW_S3_doc/zh/": [
                {
                    "url": "/PicoW_S3_doc/zh_tw/",
                    "src": "docs/PicoW_S3_doc/zh_tw"
                },
                {
                    "url": "/PicoW_S3_doc/en/",
                    "src": "docs/PicoW_S3_doc/en"
                }
            ],
            "/Pico_RP2040_doc/zh/": [
                {
                    "url": "/Pico_RP2040_doc/zh_tw/",
                    "src": "docs/Pico_RP2040_doc/zh_tw"
                },
                {
                    "url": "/Pico_RP2040_doc/en/",
                    "src": "docs/Pico_RP2040_doc/en"
                }
            ],
            "/WebBit_dev/zh/": [
                {
                    "url": "/WebBit_dev/zh_tw/",
                    "src": "docs/WebBit_dev/zh_tw"
                },
                {
                    "url": "/WebBit_dev/en/",
                    "src": "docs/WebBit_dev/en"
                }
            ],
            "/WebBit_doc/zh/": [
                {
                    "url": "/WebBit_doc/zh_tw/",
                    "src": "docs/WebBit_doc/zh_tw"
                },
                {
                    "url": "/WebBit_doc/en/",
                    "src": "docs/WebBit_doc/en"
                }
            ],
            "/Bit_S2_doc/zh/": [
                {
                    "url": "/Bit_S2_doc/zh_tw/",
                    "src": "docs/Bit_S2_doc/zh_tw"
                },
                {
                    "url": "/Bit_S2_doc/en/",
                    "src": "docs/Bit_S2_doc/en"
                }
            ],
            "/WebAI/zh/": [
                {
                    "url": "/WebAI/zh_tw/",
                    "src": "docs/WebAI/zh_tw"
                },
                {
                    "url": "/WebAI/en/",
                    "src": "docs/WebAI/en"
                }
            ],
            "/About/zh/": [
                {
                    "url": "/About/zh_tw/",
                    "src": "docs/About/zh_tw"
                },
                {
                    "url": "/About/en/",
                    "src": "docs/About/en"
                }
            ]
        },
        "pages": {
            "/": [
                {
                    "url": "/zh_tw/",
                    "src": "pages/index/zh_tw"
                },
                {
                    "url": "/en/",
                    "src": "pages/index/en"
                }
            ]
        }
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser":{
            "from": "pypi",
            "config": {
                "toc_depth": 4
            }
        },
        "teedoc-plugin-jupyter-notebook-parser": {
            "from": "pypi",
            "config": {

            }
        },
        "teedoc-plugin-google-translate": {
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-blog": {
            "from": "pypi",
            "config": {
            }
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "env":{
                    "main_color": "#225378"
                }
            }
        },
        "teedoc-plugin-search":{
            "from": "pypi",
            "config": {
                "search_hint": "Search",
                "env":{
                    "main_color": "#225378",
                    "main_color_dark": "#923732",
                    "hint_shadow_color": "rgba(146, 55, 50, 0.38)"
                }
            }
        },
        "teedoc-plugin-baidu-tongji":{
            "from": "pypi",
            "config": {
                "code": "789a62aac0b25ad52089ddb4f85128cd"
            }
        },
        "teedoc-plugin-google-analytics":{
            "from": "pypi",
            "config": {
                "id": "G-MRHWDDNYB6"
            }
        },
        "teedoc-plugin-comments-gitalk": {
            "from": "pypi",
            "config": {
                "env": {
                    "clientID": "a49cf8907bbf60cd5e06",
                    "clientSecret": "830239001737febb2419f5446f9a8518d240833b",
                    "repo": "BPI-Doc",
                    "owner": "BPI-STEAM",
                    "admin": ["BigQubot", ""],
                    "main_color": "#006DB8",
                    "second_color": "#225378"
                }
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env":{
                    "main_color": "#225378",
                    "second_color": "#e25d55"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots":{
        "User-agent": "*"
    },
    "teedoc-plugin-google-translate": {
        "from": "pypi",
        "config": {
        }
    }
}
